{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-useless-constructor": "off"
    },
    "parser": "babel-eslint",
    "plugins": ["prettier", "react"]
}
